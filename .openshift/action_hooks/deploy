CONFIG_FILE="${OPENSHIFT_DATA_DIR}config.coffee"
GAME_FILE="${OPENSHIFT_DATA_DIR}game.js"

echo "++++ Deploy ++++"

echo "rm $OPENSHIFT_REPO_DIR/config.coffee"
rm $OPENSHIFT_REPO_DIR/config.coffee

echo "ln -s $CONFIG_FILE $OPENSHIFT_REPO_DIR"
ln -s $CONFIG_FILE $OPENSHIFT_REPO_DIR

echo "ln -s $GAME_FILE $OPENSHIFT_REPO_DIR"
ln -s $GAME_FILE $OPENSHIFT_REPO_DIR
