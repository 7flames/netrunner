CONFIG_FILE="${OPENSHIFT_REPO_DIR}config.coffee"
GAME_FILE="${OPENSHIFT_REPO_DIR}game.js"

echo "++++ Deploy ++++"

echo "rm $CONFIG_FILE"
rm $CONFIG_FILE

echo "rm $GAME_FILE"
rm $GAME_FILE

echo "ln -s $CONFIG_FILE $OPENSHIFT_REPO_DIR"
ln -s $CONFIG_FILE $OPENSHIFT_REPO_DIR

echo "ln -s $GAME_FILE $OPENSHIFT_REPO_DIR"
ln -s $GAME_FILE $OPENSHIFT_REPO_DIR
